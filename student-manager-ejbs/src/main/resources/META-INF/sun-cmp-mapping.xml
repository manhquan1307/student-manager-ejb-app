<sun-cmp-mappings>
    <sun-cmp-mapping>
        <ejb-name>StudentBean</ejb-name>
        <table-name>Student</table-name>
        <cmp-field-mapping>
            <field-name>studentId</field-name>
            <column-name>studentId</column-name>
        </cmp-field-mapping>
        <cmp-field-mapping>
            <field-name>name</field-name>
